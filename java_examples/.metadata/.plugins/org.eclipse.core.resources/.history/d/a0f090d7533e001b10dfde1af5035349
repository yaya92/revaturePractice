package eg1;

public class Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 * java provides 8 primitive data types
		 * 
		 * datatype				size				defaultValue
		 * ------------------------------------------------------------
		 * byte					1byte					0
		 * short				2						0
		 * int					4						0	
		 * long					8						0 any long end it with L
		 * 
		 * 
		 * float				4						0.0000f  any float variable ends with f 
		 * double				8						0.0000   java takes all the decimal values as double by default 
		 * 
		 * char					2						'\u00000' java supports UNI-Code
		 * 
		 * boolean				1bite					false
		 * 
		 */
		
		int x = 10;
		System.out.println("x= " +x);
		
		float z = 123.55f;
		System.out.println("z= "+z);
		
		long contact = 9898978798978L;
		System.out.println("contact : "+contact);
		
		String s1 = "Angel";
		System.out.println("s1 :" +s1);
		
		double d = 23.455;
		System.out.println("d = "+d);
		
		// do research on arithmetic and relational operators and try them 
		
		
		int t = 5;
		int y = 10;
		 System.out.println(t+y);
		
	}

}
